{
  "name": "@baidu/cz-conventional-changelog-befe",
  "version": "1.3.2",
  "description": "cz-conventional-changelog preset for baidu BEFE.",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "version": "npm run changelog",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "pretty-quick --staged",
    "prepublishOnly": "npm test",
    "changelog": "conventional-changelog -p @baidu/befe -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "ssh://g@gitlab.baidu.com:8022/be-fe/cz-conventional-changelog-befe.git"
  },
  "keywords": [
    "git-cz",
    "conventional-changelog",
    "cz-conventional-changelog-befe",
    "cz-conventional-changelog",
    "befe"
  ],
  "author": "teeeemoji",
  "license": "MIT",
  "dependencies": {
    "@baidu/conventional-commit-types-befe": "^1.0.2",
    "@baidu/icafe-api": "^1.2.0",
    "@baidu/normalize-icafe-pkg": "^1.0.2",
    "@baidu/tranz-commit-icafe": "^1.0.2",
    "@moyuyc/inquirer-autocomplete-prompt": "^1.3.0",
    "chalk": "^2.4.1",
    "cli-table3": "^0.5.1",
    "cli-width": "^2.2.0",
    "find-up": "^3.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.0",
    "inquirer-store": "^1.1.1",
    "is-primitive": "^3.0.1",
    "loader-utils": "^1.1.0",
    "memoize-one": "^4.0.2",
    "minimist": "^1.2.0",
    "please-upgrade-node": "^3.1.1",
    "read-pkg-up": "^4.0.0",
    "shallowequal": "^1.1.0",
    "strip-ansi": "^5.0.0",
    "terminal-link": "^1.1.0",
    "tiny-i18n": "^1.1.2",
    "tranz": "^0.6.0",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@baidu/commitlint-config-befe": "^1.2.1",
    "@baidu/conventional-changelog-befe": "^1.3.1",
    "@commitlint/cli": "^7.1.1",
    "@types/jest": "^23.3.5",
    "commitizen": "^3.0.2",
    "conventional-changelog-cli": "^2.0.5",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0"
  },
  "lang": "zh",
  "icafe": {
    "spaceId": "befe-erp"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    },
    "@baidu/cz-conventional-changelog-befe": {}
  },
  "engines": {
    "node": ">=7"
  },
  "commitlint": {
    "extends": [
      "@baidu/commitlint-config-befe"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  }
}
